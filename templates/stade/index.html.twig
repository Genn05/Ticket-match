{% extends 'base.html.twig' %}

{% block title %}Liste des Stades{% endblock %}

{% block nav %}
    {% include 'partials/nav.html.twig' %}
{% endblock %}

{% block body %}
<div class="container py-4">

    <h1 class="mb-4">Liste des Stades</h1>
   <!-- Section STADES POPULAIRES -->
    <div class="mt-5">
        <h2 class="mb-4 text-uppercase position-relative d-inline-block">
            STADES POPULAIRES
            <span class="position-absolute" style="height: 3px; width: 60px; background-color: #007bff; bottom: -5px; left: 0;"></span>
        </h2>

        <div class="row">
            <div class="col-md-6 col-lg-4 mb-4">
                <div class="card shadow stadium-card border-0 h-100">
                    <img src="data:image/jpeg;base64," class="card-img-top stadium-image" alt="Parc des Princes">
                    <div class="card-body">
                        <h5 class="card-title fw-bold">Parc des Princes</h5>
                        <p class="card-text"><i class="fas fa-map-marker-alt me-2"></i> Paris, France</p>
                        <p class="card-text text-muted">Capacité : 48 000 places</p>
                    </div>
                </div>
            </div>

            <!-- Répéter cette carte pour d'autres stades populaires -->
            <div class="col-md-6 col-lg-4 mb-4">
                <div class="card shadow stadium-card border-0 h-100">
                    <img src="data:image/jpeg;base64," class="card-img-top stadium-image" alt="Stade Vélodrome">
                    <div class="card-body">
                        <h5 class="card-title fw-bold">Stade Vélodrome</h5>
                        <p class="card-text"><i class="fas fa-map-marker-alt me-2"></i> Marseille, France</p>
                        <p class="card-text text-muted">Capacité : 67 000 places</p>
                    </div>
                    
                </div>
                
            </div>
<div class="col-md-6 col-lg-4 mb-4">
                <div class="card shadow stadium-card border-0 h-100">
                    <img src="data:image/jpeg;base64," class="card-img-top stadium-image" alt="Stade Vélodrome">
                    <div class="card-body">
                        <h5 class="card-title fw-bold">Stade Camp Now</h5>
                        <p class="card-text"><i class="fas fa-map-marker-alt me-2"></i> barcleone, espange</p>
                        <p class="card-text text-muted">Capacité : 98 000 places</p>
                    </div>
        </div>
    </div>
</div>
    <table class="table table-bordered table-hover">
        <thead class="table-dark">
            <tr>
                <th>ID</th>
                <th>Nom</th>
                <th>Ville</th>
                <th>Capacité</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for stade in stades %}
            <tr>
                <td>{{ stade.id }}</td>
                <td>{{ stade.nom }}</td>
                <td>{{ stade.ville }}</td>
                <td>{{ stade.capacite }}</td>
                <td>
                    <a href="{{ path('app_stade_show', {'id': stade.id}) }}" class="btn btn-sm btn-primary me-2">Voir</a>
                    {% if is_granted('ROLE_ADMIN') %}
                    <a href="{{ path('app_stade_edit', {'id': stade.id}) }}" class="btn btn-sm btn-warning">Modifier</a>
                    {% endif %}
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5" class="text-center">Aucun stade trouvé.</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% if is_granted('ROLE_ADMIN') %}
    <a href="{{ path('app_stade_new') }}" class="btn btn-success mt-3">Ajouter un nouveau stade</a>
 {% endif %}
 

{% include 'partials/footer.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <style>
        /* CSS personnalisé spécifique à cette page */
        .stadium-card {
            transition: transform 0.3s ease;
        }

        .stadium-card:hover {
            transform: scale(1.03);
        }

        .stadium-image {
            object-fit: cover;
            height: 200px;
            width: 100%;
        }

        .btn-primary, .btn-warning {
            margin: 0 5px;
        }

        .table-dark th, .table-dark td {
            background-color: #343a40;
            color: #fff;
        }
    </style>
{% endblock %}
{% endblock %}
